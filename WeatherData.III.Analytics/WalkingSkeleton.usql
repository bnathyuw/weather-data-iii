USE DATABASE [WeatherData];

REFERENCE ASSEMBLY [Objects];

USING Extractors = WeatherData.[III].Objects.Extractors;
USING Outputters = WeatherData.[III].Objects.Outputters;

@data =
    EXTRACT value int
    FROM "input\\walkingSkeleton\\data.txt"
    USING Extractors.WalkingSkeleton;

OUTPUT @data
TO "output\\walkingSkeleton\\data.txt"
USING sOutputters.WalkingSkeleton;